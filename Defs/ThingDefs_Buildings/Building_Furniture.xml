<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- SQUARE FLOOR LIGHTS -->

  <ThingDef Name="FloorLightSquare" ParentName="BuildingBase" Abstract="True">
    <thingClass>Building</thingClass>
    <altitudeLayer>Floor</altitudeLayer>
    <passability>Standable</passability>
    <building>
      <isInert>true</isInert>
      <isEdifice>false</isEdifice>
    </building>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <WorkToBuild>300</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Beauty>2</Beauty>
    </statBases>
    <selectable>true</selectable>
    <size>(1,1)</size>
    <costList>
      <Steel>12</Steel>
    </costList>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <designationCategory>Lamps</designationCategory>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <damageData>
        <rect>(0.2,0.2,0.6,0.6)</rect>
      </damageData>
    </graphicData>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>24</basePowerConsumption>
        <shortCircuitInRain>false</shortCircuitInRain>
      </li>
      <li Class="CompProperties_Flickable"/>
    </comps>
    <rotatable>false</rotatable>
    <clearBuildingArea>false</clearBuildingArea>
    <researchPrerequisites><li>Electricity</li></researchPrerequisites>
  </ThingDef>

  <ThingDef ParentName="FloorLightSquare">
    <defName>FloorLightSquare</defName>
    <label>floor light (square)</label>
    <description>Square floor light integraded into the ground. Waterproof and quite pretty.</description>
    <graphicData>
      <texPath>Things/Building/Lamps/FloorLightSquare</texPath>
    </graphicData>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>12</glowRadius>
        <glowColor>(217,217,208,0)</glowColor>
        <colorPickerEnabled>true</colorPickerEnabled>
        <darklightToggle>true</darklightToggle>
      </li>
    </comps>
  </ThingDef>


